<%= content_tag_for :li, sub_image do %>
  <%= image_tag sub_image.image_url(:thumb).to_s, class: 'handle' %>
  <%= best_in_place [sub_image.project, sub_image], :caption %>
  <%= link_to "", [sub_image.project, sub_image], method: :delete, data: { confirm:'Are you sure? '}, class: "glyphicon glyphicon-trash" %>
<% end %>
